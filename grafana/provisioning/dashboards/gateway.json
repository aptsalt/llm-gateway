{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": true,
  "panels": [
    {
      "title": "Requests per Second",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(gateway_requests_total[5m])",
          "legendFormat": "{{provider}} - {{model}} ({{status}})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "Request Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(gateway_latency_seconds_bucket[5m]))",
          "legendFormat": "{{provider}} - {{model}} p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "Total Cost",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(gateway_cost_dollars)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      }
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "targets": [
        {
          "expr": "sum(gateway_cache_hits_total) / (sum(gateway_cache_hits_total) + sum(gateway_cache_misses_total))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.2 },
              { "color": "green", "value": 0.4 }
            ]
          }
        }
      }
    },
    {
      "title": "Active Providers",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "gateway_active_providers",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 3 }
            ]
          }
        }
      }
    },
    {
      "title": "Error Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(gateway_requests_total{status!=\"200\"}[5m])) / sum(rate(gateway_requests_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          }
        }
      }
    },
    {
      "title": "Tokens by Provider",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "rate(gateway_tokens_total[5m])",
          "legendFormat": "{{provider}} {{direction}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "bars", "fillOpacity": 50 }
        }
      }
    },
    {
      "title": "Cost by Provider",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "rate(gateway_cost_dollars[5m])",
          "legendFormat": "{{provider}} - {{model}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "Circuit Breaker State",
      "type": "state-timeline",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "gateway_circuit_breaker_state",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 70 },
          "mappings": [
            { "options": { "0": { "text": "Closed", "color": "green" } }, "type": "value" },
            { "options": { "1": { "text": "Half-Open", "color": "yellow" } }, "type": "value" },
            { "options": { "2": { "text": "Open", "color": "red" } }, "type": "value" }
          ]
        }
      }
    },
    {
      "title": "Latency Distribution",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 26 },
      "targets": [
        {
          "expr": "gateway_latency_seconds_bucket",
          "legendFormat": "{{provider}} {{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Routing Strategy Distribution",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 26 },
      "targets": [
        {
          "expr": "sum by(routing_strategy) (gateway_requests_total)",
          "legendFormat": "{{routing_strategy}}",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["llm", "gateway"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "title": "LLM Gateway Overview",
  "uid": "llm-gateway-overview",
  "version": 1,
  "refresh": "10s"
}
